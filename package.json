{
  "name": "link-sweeper",
  "version": "1.3.1",
  "description": "Link Sweeper is a simple command-line tool designed to keep your web application's links fresh and functional. Tailored for modern web projects, it supports a variety of frameworks such as React, Vue, Svelte, and more, ensuring your project stays link-error-free.",
  "homepage": "https://github.com/benjamindigiorgio/link-sweeper",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "build": "unbuild"
  },
  "bin": {
    "link-sweeper": "./dist/cli.mjs"
  },
  "build": {
    "entries": [
      "./src/cli",
      "./src/index"
    ]
  },
  "keywords": [
    "links",
    "testing",
    "test",
    "web",
    "react",
    "vue",
    "html",
    "svelte",
    "astro",
    "solid-js"
  ],
  "author": "Benjamin Di Giorgio",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "axios": "^1.6.8",
    "babel-jest": "^29.7.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/babel__traverse": "^7.20.5",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@types/yargs": "^17.0.32",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "unbuild": "^2.0.0"
  }
}
